{
  "displayName":"根",
  "url":"/**",
  "submodule": {
    "tag": {
      "displayName": "标签管理",
      "url": "/tags/**"
    },
    "thing": {
      "displayName": "设备管理",
      "url": "/things/**"
    },
    "usergroup":{
      "displayName": "设备管理",
      "url": "/usergroup/**"
    },
    "triggers":{
      "displayName":"触发器管理",
      "url":"/triggers/**"
    }
    "user": {
      "displayName": "用户信息",
      "url": "/users/**",
      "submodule": {
        "UpdateInfo": {
          "method": "PATCH",
          "url": "/users/me"
        },
        "GetInfo": {
          "method": "GET",
          "url": "/users/me"
        },
        "changePwd": {
          "method": "POST",
          "url": "/users/changepassword"
        },
        "queryInfo":{
          "method": "GET",
          "url": "/users/*"
        }
      }
    },
    "usermanager": {
      "displayName": "用户管理",
      "url": "/usermanager/**",
      "submodule": {
        "CreateUser": {
          "method": "POST",
          "url": "/usermanager"
        },
        "ResetPassword": {
          "method": "POST",
          "url": "/usermanager/*/resetpassword"
        },
        "UpdateUser": {
          "method": "PATCH",
          "url": "/usermanager/*"
        },
        "GetUser": {
          "method": "GET",
          "url": "/usermanager/*"
        }
      }
    },
    "auth": {
      "displayName": "登录管理",
      "url": "/oauth2/*",
      "submodule": {
        "ActiviteUser": {
          "method": "POST",
          "url": "/oauth2/activate"
        },
        "ResetPassword": {
          "method": "POST",
          "url": "/oauth2/initpassword"
        },
        "Login": {
          "method": "POST",
          "url": "/oauth2/login"
        },
        "Logout": {
          "method": "POST",
          "url": "/oauth2/logout"
        },
        "validatetoken": {
          "method": "POST",
          "url": "/oauth2/validatetoken"
        }
      }
    },
    "system": {
      "displayName": "系统维护",
      "url":"/sys/**",
      "submodule": {
        "AppEnvInit": {
          "method": "POST",
          "url": "/sys/appinit"
        },
        "AppRegist": {
          "method": "POST",
          "displayName":"添加新kiiCloud app",
          "url": "/sys/appRegist/*"
        }
      }
    }
  }
}
