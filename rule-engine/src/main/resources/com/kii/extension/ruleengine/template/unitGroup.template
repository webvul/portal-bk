

rule "${triggerID} multiple segment:group express ${name}"

when
    $ext:ExternalCollect()
	Trigger($triggerID:triggerID=="${triggerID}" , enable == true)
    $summary:Summary( triggerID==$triggerID , $things:things ,name=="${name}" )
    $status:ThingStatusInRule( $thingID:thingID memberOf $things,${express}  )
then
	System.out.println("gener member match result"+$summary+" "+$thingID);
	insertLogical(new MemberMatchResult($summary,$status));
end
